{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api-docs/(.*)", 
        "dest": "public/api-docs/$1"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }