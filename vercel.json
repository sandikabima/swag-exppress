{
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/api-docs/(.*)",
        "status": 200,
        "headers": {
          "Content-Type": "text/html; charset=UTF-8"
        },
        "continue": true
      }
    ]
  }